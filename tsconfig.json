{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "lib": ["es2017", "dom"],
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@ngxs-labs/data": ["ngxs/data/src/public-api.ts"]
    }
  }
}
